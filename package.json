{
  "name": "firstbot",
  "version": "0.1.0",
  "description": "a discord bot used to learn the api",
  "main": "build/index.ts",
  "engines": {
    "node": "10.x"
  },
  "scripts": {
    "build-ts": "tsc",
    "postinstall": "[ -f /etc/resolv.conf ] && echo 'Config Exists' || cp src/configTemplate.ts src/config.ts || npm run build-ts",
    "start": "npm run serve",
    "serve": "node build/index.js",
    "watch-node": "nodemon dist/server.js",
    "watch-ts": "tsc -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gramanicu/fistDiscordBot.git"
  },
  "author": "Grama Nicolae",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gramanicu/fistDiscordBot/issues"
  },
  "homepage": "https://github.com/gramanicu/fistDiscordBot#readme",
  "dependencies": {
    "@types/node": "^12.7.5",
    "discord.js": "^11.5.1",
    "typescript": "^3.6.3"
  }
}
